{
  "extends": "airbnb",
  "rules": {
    "object-curly-newline": "off",
    "import/no-dynamic-require": "off",
    "func-names": "off",
    "max-len": "off",
    "no-lone-blocks": "off",
    "semi": "off",
    "one-var": "off",
    "one-var-declaration-per-line": "off",
    "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }]
  }
}
